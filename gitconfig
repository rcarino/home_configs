[alias]
	co = checkout
	st = status
	# apply stash by alias name
	sapply = "!f() { git stash apply stash^{/$*}; }; f"
	amend = commit --amend
	cp = cherry-pick
	br = branch
	ci = commit
	nuke = !git clean -fd && git checkout .
[push]
	default = simple
[user]
	name = Raymond Carino
	email = rcarino@classpass.com
[core]
	excludesfile = ~/.gitignore
