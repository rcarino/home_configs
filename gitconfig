[alias]
	co = checkout
	st = status
	# apply stash by alias name
	sapply = "!f() { git stash apply stash^{/$*}; }; f"
	amend = commit --amend
	cp = cherry-pick
	br = branch
	co = pull
	ci = commit
[push]
	default = simple
